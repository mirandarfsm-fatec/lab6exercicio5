<?xml version="1.0" encoding="UTF-8"?>
<pretty-config xmlns="http://ocpsoft.org/schema/rewrite-config-prettyfaces"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://ocpsoft.org/schema/rewrite-config-prettyfaces
                      http://ocpsoft.org/xml/ns/prettyfaces/rewrite-config-prettyfaces.xsd">

	<url-mapping id="cadastro-planilha-lista">
		<pattern value="/contagem/planilha/" />
		<view-id value="/pages/contagem/planilha/cadastro-planilha-lista.jsf" />
	</url-mapping>
	
	<url-mapping id="cadastro-planilha-lista-criada">
		<pattern value="/contagem/planilha/criada/" />
		<view-id value="/pages/contagem/planilha/cadastro-planilha-lista-criada.jsf" />
	</url-mapping>
	
	<url-mapping id="cadastro-planilha-lista-em-revisao">
		<pattern value="/contagem/planilha/em-revisao/" />
		<view-id value="/pages/contagem/planilha/cadastro-planilha-lista-em-revisao.jsf" />
	</url-mapping>
	
	<url-mapping id="cadastro-planilha-lista-revisada">
		<pattern value="/contagem/planilha/revisada/" />
		<view-id value="/pages/contagem/planilha/cadastro-planilha-lista-revisada.jsf" />
	</url-mapping>
	
	<url-mapping id="cadastro-planilha-lista-em-aprovacao">
		<pattern value="/contagem/planilha/em-aprovacao/" />
		<view-id value="/pages/contagem/planilha/cadastro-planilha-lista-em-aprovacao.jsf" />
