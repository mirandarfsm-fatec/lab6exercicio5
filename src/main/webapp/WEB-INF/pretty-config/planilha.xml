		<pattern value="/contagem/planilha/aprovada/" />
		<view-id value="/pages/contagem/planilha/cadastro-planilha-lista-aprovada.jsf" />
	</url-mapping>

	<url-mapping id="cadastro-planilha-lista-invalidada">
		<pattern value="/contagem/planilha/invalidada/" />
		<view-id value="/pages/contagem/planilha/cadastro-planilha-lista-invalidada.jsf" />
	</url-mapping>
	
	<url-mapping id="cadastro-planilha-formulario">
		<pattern value="/contagem/planilha/#{id}" />
		<view-id value="/pages/contagem/planilha/cadastro-planilha-formulario.jsf" />
	</url-mapping>

	<url-mapping id="cadastro-planilha-formulario-detalhes">
		<pattern value="/contagem/planilha/detalhes/#{id}" />
		<view-id value="/pages/contagem/planilha/cadastro-planilha-detalhes.jsf" />
	</url-mapping>
	
	<url-mapping id="cadastro-item-planilha-lista">
		<pattern value="/contagem/planilha/itens/#{id}" />
		<view-id value="/pages/contagem/planilha/itemplanilha/cadastro-item-planilha-formulario.jsf" />
	</url-mapping>
	
	<url-mapping id="cadastro-item-planilha-lista-detalhes">
		<pattern value="/contagem/planilha/itens/detalhes/#{id}" />
		<view-id value="/pages/contagem/planilha/itemplanilha/cadastro-item-planilha-detalhes.jsf" />
	</url-mapping>
	
</pretty-config>